{
    "manifest_version": 2,

    "name": "DHIS2",
    "description": "DHIS2 Chrome Offline Application",
    "version": "0.0.1",
    "icons": {
        "128": "img/icon.png"
    },
    "app": {
        "background": {
            "page": "background.html"
        }
    },
    "permissions": ["alarms", "unlimitedStorage", "https://*/", "http://*/", {
        "fileSystem": ["write", "retainEntries", "directory"]
    }]
}
